# Personal account (@ceiphr, default)
Host github.com
   HostName github.com
   User git
   IdentityFile ~/.ssh/ceiphr.pub
   IdentitiesOnly yes

# Work account (@aribirnbaum)
Host github.com-aribirnbaum
   HostName github.com
   User git
   IdentityFile ~/.ssh/aribirnbaum.pub
   IdentitiesOnly yes

Host *
  IdentityAgent ~/.1password/agent.sock
